{
  "name": "Neef",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#9B00FF" // ม่วงเข้ม
    },
    "variable.constant": {
      "foreground": "#06D001" // เขียว
    },
    "variable.defaultLibrary": {
      "foreground": "#9B00FF" // ฟ้าไซเบอร์
    }
  },
  "tokenColors": [
    {
      "name": "Edit foreground (e.g., (), {}, ;)",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Variable, Properties",
      "scope": [
        "variable",
        "entity.name.variable",
        "meta.object-literal.key",
        "support.variable"
      ],
      "settings": {
        "foreground": "#06D001"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9B00FF"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#9B00FF"
      }
    },
    {
      "name": "Class, Constants",
      "scope": [
        "entity.name.type",
        "constant.language",
        "constant.other",
        "support.class"
      ],
      "settings": {
        "foreground": "#9B00FF" // เปลี่ยนจาก #FF0040 เป็นม่วงเข้มโทนเย็น
      }
    },
    {
      "name": "Function, Method",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#9B00FF"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#06D001"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Comments",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "foreground": "#222222"
      }
    },
    {
      "name": "File names (e.g., in explorer)",
      "scope": ["entity.name.filename"],
      "settings": {
        "foreground": "#9B00FF"
      }
    }
  ],
